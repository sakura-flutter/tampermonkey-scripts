    "predeploy": "npm run build",
    "deploy": "gh-pages -d ./dist",
    "lint": "eslint ./ --ext .js,.ts,.tsx",
    "stylelint": "stylelint **/*.scss",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sakura-flutter/tampermonkey-scripts.git"
  },
