    "terser-webpack-plugin": "^5.3.3",
    "typescript": "^4.7.4",
    "viewerjs": "^1.10.5",
    "vue": "^3.2.37",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run lint"
